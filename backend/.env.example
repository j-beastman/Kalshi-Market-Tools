# Kalshi API Configuration
KALSHI_API_KEY=your-api-key-here
KALSHI_PRIVATE_KEY=your-private-key-pem-content-here

# Database Configuration (Railway will provide this automatically)
DATABASE_URL=postgresql://user:password@host:port/dbname

# Redis Configuration (Railway will provide this automatically)
REDIS_URL=redis://host:port

# API Configuration
ENVIRONMENT=production
CORS_ORIGINS=https://yourusername.github.io,http://localhost:3000

# Cache Configuration
CACHE_UPDATE_INTERVAL=30
CACHE_RETENTION_DAYS=7
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_PERIOD=3600

# Optional: News API for enhanced detection
NEWS_API_KEY=your-news-api-key
ENABLE_NEWS_DETECTION=false
